{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "diskStorageType": {
            "value": "Standard_LRS"
        },
        "vmAdminUsername": {
            "value": "azureuser"
        },
        "vmAdminPassword": {
            "value": null
        },
        "installCloudLabsShadow": {
            "value": "yes"
        },
        "location": {
            "value": "northeurope"
        },
        "dbusername": {
            "value": "sqluser"
        },
        "dbpasswd": {
            "value": null
